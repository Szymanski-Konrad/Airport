<Window x:Class="Airport.Page.FirmWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Airport.Page"
        mc:Ignorable="d"
        Title="FirmWindow" Height="450" Width="800">
    <DockPanel>
        <WrapPanel Name="FirmWrap" DockPanel.Dock="Top" Height="50" HorizontalAlignment="Center">
            <Button Content="Wroc do menu" Margin="10"/>
            <TextBlock Text="Nazwa firmy: " Margin="10"/>
            <TextBlock Text="Konto firmy: " Margin="10"/>
            <Button Margin="5" Content="Lotniska" Click="AirportPage_Click"/>
            <Button Margin="5" Content="Loty" Click="FlightPage_Click"/>
            <Button Margin="5" Content="Promocje" Click="Discount_Click"/>
            <Button Margin="5" Content="Samoloty" Click="Planes_Click"/>
        </WrapPanel>
        <Frame Name="frame" Content="" HorizontalAlignment="Left" NavigationUIVisibility="Hidden" Height="360" VerticalAlignment="Top" Width="800"/>
    </DockPanel>
</Window>
