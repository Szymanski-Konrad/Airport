<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Airport"
                   namespace="Airport.Model"
                   auto-import="true">
  <class name="Discount" table="Discount" lazy="false">
    <id name="id">
      <generator class="native" />
    </id>
    <property name="dateDeparture" column="dateDeparture"/>
    <property name="dateArrival" column="dateArrival" />
    <property name="value" column="value" />
  </class>
</hibernate-mapping>